{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/LCbox.html"
    },
    {
      "src": "/LCbox_retrieve",
      "dest": "/LCbox_retrieve.html"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
